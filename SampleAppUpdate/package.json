  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-app-update": ".."
  },
  "devDependencies": {
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "jest": "23.1.0",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
